    "clsx": "^2.0.0",
    "framer-motion": "^10.16.16",
    "gtag": "^1.0.1",
    "i18next": "^23.16.8",
    "i18next-browser-languagedetector": "^7.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-gtm-module": "^2.0.11",
    "react-hook-form": "^7.48.2",
    "react-i18next": "^13.5.0",
